{
  "name": "zk-coup",
  "version": "1.0.0",
  "description": "A Phaser 3 game template using Parcel bundler",
  "main": "src/index.html",
  "scripts": {
    "build": "parcel build src/index.html --public-url ./ --no-source-maps",
    "watch": "parcel watch src/index.html",
    "start": "parcel serve src/index.html --no-hmr",
    "clean": "rmdir /s /q dist",
    "test": "npx eslint src"
  },
  "keywords": [
    "phaser",
    "phaser3",
    "phaser-boilerplate"
  ],
  "author": "hsaka",
  "license": "ISC",
  "dependencies": {
    "fetch-retry": "^5.0.6",
    "jquery": "^3.7.0",
    "jsonrepair": "^3.2.0",
    "localforage": "^1.10.0",
    "phaser": "^3.61.0-beta.3",
    "plugins": "^0.4.2",
    "sweetalert2": "^11.7.12",
    "utils": "^0.3.1"
  },
  "devDependencies": {
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-config-semistandard": "^13.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "parcel-plugin-static-files-copy": "^2.6.0"
  }
}
